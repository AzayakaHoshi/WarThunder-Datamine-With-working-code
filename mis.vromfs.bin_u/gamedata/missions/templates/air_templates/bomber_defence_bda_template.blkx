      comment{
        value:t=""
      }

      comment{
        value:t=""
      }

      comment{
        value:t=""
      }

      unitMoveTo{
        target:t="retreat_point"
        follow_target:b=no
        teleportHeightType:t="absolute"
        useUnitHeightForTele:b=yes
        shouldKeepFormation:b=yes
        teleportHeightValue:r=1000.0
