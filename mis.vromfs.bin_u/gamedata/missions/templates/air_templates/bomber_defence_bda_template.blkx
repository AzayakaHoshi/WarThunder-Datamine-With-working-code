    events{

      initMission{
      }
    }

    conditions{
    }

    actions{

      varSetInt{
        value:i=0
        var:t="bda_bombers_count"
        input_var:t="bda_ai_max_count"
      }

      varSetInt{
        value:i=0
        var:t="bda_tickets_for_1_kill"
        source_var:t="mpTi1"
      }

      unitWakeUp{
        target:t="t1_ai_bombers"
        varNameNumToWakeUp:t="bda_ai_max_count"
