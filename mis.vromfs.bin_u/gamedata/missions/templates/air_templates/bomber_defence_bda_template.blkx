triggers{
  isCategory:b=yes
  is_enabled:b=yes

  bda_template_init{
    is_enabled:b=yes
    comments:t=""

    props{
      actionsType:t="PERFORM_ONE_BY_ONE"
      conditionsType:t="ALL"
      enableAfterComplete:b=no
    }

    events{

      initMission{
      }
    }

    conditions{
    }

    actions{

      varSetInt{
        value:i=0
        var:t="bda_ai_max_count"
        fromDescriptor:t="aiTargetCount_teamA"
      }

      varSetInt{
        value:i=0
        var:t="bd_timer_from_desc"
        fromDescriptor:t="bd_timer"
      }

      missionMarkAsRespawnPoint{
        loc_name:t="missions/friendly_bombers_spawn"
        spawnEffect:b=no
        isStrictSpawn:b=no
        isAirfield:b=no
        isUnit:b=yes
        useExisting:b=no
        isIndividual:b=no
        removeAreas:b=no
        canSpawnOnNeutral:b=no
        showOnMap:b=no
        offset:p3=-1000.0, 500.0, 0.0
        radius:r=250.0
        onlyOnePlayerPerSpawnPoint:b=no
        target:t="t1_ai_bombers"
        team:t="A"
        backupZone:t="t1_air_spawn"
      }

      missionMarkAsRespawnPoint{
        loc_name:t="missions/enemy_bombers_spawn"
        spawnEffect:b=no
        isStrictSpawn:b=no
        isAirfield:b=no
        isUnit:b=yes
        useExisting:b=no
        isIndividual:b=no
        removeAreas:b=no
        canSpawnOnNeutral:b=no
