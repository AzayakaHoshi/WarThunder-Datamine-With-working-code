    actions{

      comment{
        value:t="vybiraem sluchajnoe sobytie"
      }

      comment{
        value:t="prinuditel~'nyj vybor shablona - dlya testov"
      }

      comment{
        value:t="proveryaem net li forsirovaniya shablona"
      }

      varSetRandomInt{
        var:t="to_randomizer"
        from:i=1
        to_not_including:i=10
        var_to_not_including:t="to_templates_quantity"
      }

      __varSetInt{
        value:i=5
        var:t="to_randomizer"
      }

      triggerEnable{
        target:t="to_check_force_template"
      }
    }

    else_actions{
    }
  }

