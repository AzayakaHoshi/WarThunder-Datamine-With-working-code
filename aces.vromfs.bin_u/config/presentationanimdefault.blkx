aircraftPresentation{
  duration:r=20.0
  looped:b=yes

  keys{

    key00{
      startTime:r=0.0
      propellersActivate:b=yes
    }

    key01{
      startTime:r=5.0
      aileron:r=1.0
      elevator:r=1.0
      rudder:r=1.0
    }

    key02{
      startTime:r=10.0
      aileron:r=-1.0
      rudder:r=1.0
    }

    key03{
      startTime:r=15.0
      elevator:r=-1.0
    }
  }
}

tankPresentation{
  duration:r=10.0
  looped:b=yes

  keys{

    key00{
      startTime:r=0.0
      gunAngles:p2=40.0, 40.0
    }

    key00{
      startTime:r=5.0
      gunAngles:p2=-40.0, 40.0
    }
  }
}