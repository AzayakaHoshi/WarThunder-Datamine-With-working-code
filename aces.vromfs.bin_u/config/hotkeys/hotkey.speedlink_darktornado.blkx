controls{
  version:i=5

  basePresetPaths{
    default:t="config/hotkeys/hotkey.speedlink_blackWidow.blk"
  }

  hotkeys{

    ID_CONTINUE{
      keyboardKey:i=57
    }

    ID_FIRE_ADDITIONAL_GUNS{
      keyboardKey:i=3
    }

    ID_FIRE_ADDITIONAL_GUNS{
      mouseButton:i=0
    }

    ID_FIRE_ADDITIONAL_GUNS{
      joyButton:i=1
    }

    ID_FIRE_CANNONS{
      keyboardKey:i=3
    }

    ID_FIRE_CANNONS{
      mouseButton:i=0
    }

    ID_FIRE_CANNONS{
      joyButton:i=1
    }

    ID_FLAPS{
      keyboardKey:i=33
    }

    ID_FLAPS{
      joyButton:i=4
    }

    ID_GEAR{
      keyboardKey:i=34
    }

    ID_GEAR{
      joyButton:i=5
    }

    ID_LOCK_TARGET{
      keyboardKey:i=22
    }

    ID_LOCK_TARGET{
      keyboardKey:i=15
    }

    ID_PTT{
      keyboardKey:i=157
    }

    ID_SCREENSHOT{
      keyboardKey:i=42
      keyboardKey:i=183
    }

    ID_TARGET_CAMERA{
      keyboardKey:i=210
    }

    ID_TARGET_CAMERA{
      mouseButton:i=1
    }

    ID_ZOOM_TOGGLE{
      keyboardKey:i=44
    }

    ID_ZOOM_TOGGLE{
      keyboardKey:i=82
    }

    ID_ZOOM_TOGGLE{
      joyButton:i=6
    }
  }

  axes{

    ailerons{
      axisId:i=1
    }

    elevator{
      axisId:i=0
    }

    rudder{
      axisId:i=-1
    }

    ship_sight_distance{
      relative:b=yes
    }

    throttle{
      axisId:i=2
    }
  }

  params{
  }

  deviceMapping{

    joystick{
      axesCount:i=3
      axesOffset:i=0
      buttonsCount:i=8
      buttonsOffset:i=0
      connected:b=yes
      devId:t="1A34:0805"
      name:t="SL-6632 Dark Tornado Joystick"
    }
  }
}

