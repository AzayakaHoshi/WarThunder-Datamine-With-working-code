fsms{

  main{
    stateInit:t="init"

    actionsTemplates{

      init{

        setEnabled{
          value:b=no
        }

        setCenterAzimuth{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setScanPatternSet{
          scanPatternSet:t="search"
        }

        setScopeRangeSet{
          scopeRangeSet:t="common"
        }

        setTransiver{
          transiver:t="common"
        }

        setSignal{
          signal:t="common"
        }

        setTargetDesignationRange{
          azimuthRange:p2=-180.0, 180.0
          azimuthWidth:r=5.0
          distanceRange:p2=0.0, 20000.0
        }
      }

      setStandbyMode{

        setModeName{
          name:t="standby"
        }

        setEnabled{
          value:b=no
        }

        setCenterElevation{
          source:t="constant"
          value:r=11.0
        }

        setScanPatternSet{
          scanPatternSet:t="search"
        }
      }

      resetStandbyMode{
      }

      setSearchMode{

        setModeName{
          name:t="search"
        }

        setEnabled{
          value:b=yes
        }

        setEnabled{
          value:b=yes
        }

        setFsmActive{
          fsm:t="tws"
          active:b=yes
        }

        setCenterElevation{
          source:t="constant"
          value:r=11.0
        }

        setScanPatternSet{
          scanPatternSet:t="search"
        }

        setCenterAzimuth{
          source:t="body"
        }

        setScanDirectionPhase{
          azimuth:r=0.0
          elevation:r=0.0
        }
      }

      resetSearchMode{

        clearTargets{
        }

        setFsmActive{
          fsm:t="tws"
          active:b=no
        }
      }
    }

    transitions{

      init{
        stateFrom:t="init"
        event:t="init"
        stateTo:t="standby"

        actions{

          init{
          }

          setStandbyMode{
          }
        }
      }

      switchOn{
        stateFrom:t="standby"
        command:t="switch"
        event:t="enable"
        stateTo:t="search"

        actions{

          setSearchMode{
          }
        }
      }

      switchScanPattern{
        stateFrom:t="standby"
        stateFrom:t="search"
        command:t="scanPatternSwitch"

        actions{

          setNextScanPattern{
          }
        }
      }

      switchScopeRange{
        command:t="rangeSwitch"

        actions{

          setNextScopeRange{
          }
        }
      }

      switchingOff{
        stateFrom:t="search"
        command:t="switch"
        event:t="disable"
        stateTo:t="antennaParking"

        actions{

          setFsmActive{
            fsm:t="parkAntenna"
            active:b=yes
          }
        }
      }

      switchOff{
        stateFrom:t="antennaParking"
        event:t="inScanDirectionPhase"
        stateTo:t="standby"

        actions{

          setFsmActive{
            fsm:t="parkAntenna"
            active:b=no
          }

          setScanDirectionPhase{
            azimuth:r=0.0
            elevation:r=0.0
          }

          setStandbyMode{
          }
        }
      }
    }
  }

  search{
    stateInit:t="search"

    transitions{

      scan{
        event:t="update"

        actions{

          scan{
          }
        }
      }

      detect{
        event:t="targetInSight"

        actions{

          setDistGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          setRelSpeedGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          detectTarget{
          }
        }
      }

      addTarget{
        event:t="targetDetected"

        actions{

          addTarget{
          }
        }
      }

      setCueElevation{
        command:t="cueAxisZ"

        actions{

          setCueElevation{
          }
        }
      }
    }
  }

  tws{
    stateInit:t="search"
    stateInit:t="search"

    transitions{

      scan{
        event:t="update"

        actions{

          scan{
          }

          scan{
            ignoreOwnWeapon:b=yes
          }

          extrapolateTargetsOfInterest{
          }

          clearTargetsOfInterest{
            timeOut:r=3.0
          }

          setCueToActiveTargetOfInterest{
          }

          setCenterAzimuth{
            source:t="constant"
            value:r=0.0
          }

          setCenterAzimuth{
            source:t="designationCue"
          }

          setCenterAzimuth{
            source:t="activeTargetOfInterest"
          }

          setCenterElevation{
            source:t="constant"
            value:r=0.0
          }

          setCenterElevation{
            source:t="designationCue"
          }

          setCenterElevation{
            source:t="activeTargetOfInterest"
          }
        }
      }

      detect{
        event:t="targetInSight"

        actions{

          setDistGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          setRelSpeedGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          detectTarget{
          }
        }
      }

      addTarget{
        event:t="targetDetected"

        actions{

          updateTargetOfInterest{
            limit:i=40
            posGateTimeInitial:p2=2.0, 4.0
            posGateRangeInitial:r=0.0
            posGateMaxTime:r=2.0
            posGateRange:p2=0.0, 1000.0
            dopplerSpeedGateRange:p2=0.0, 1600.0
            dopplerSpeedGateMaxTime:r=8.0

            dirFilter{
              alpha:r=0.5
              betta:r=1.2
              dt:r=0.0208
              rateLim:p2=0.5, 0.5
              limTimeOut:r=3.0
            }

            distFilter{
              alpha:r=0.5
              betta:r=0.05
              dt:r=0.0208
              limTimeOut:r=3.0
            }

            dopplerSpeedFilter{
              alpha:r=0.25
              betta:r=0.05
              dt:r=0.0208
              limTimeOut:r=3.0
            }
          }

          updateActiveDetectedTarget{
          }

          setCueToActiveTargetOfInterest{
          }
        }
      }

      switchSelectedTarget{
        command:t="switchTarget"

        actions{

          setNextDetectedTargetActive{
          }
        }
      }

      setCueAzimuth{
        command:t="cueAxisX"

        actions{

          setCueAzimuth{
          }
        }
      }

      setCueDist{
        command:t="cueAxisY"

        actions{

          setCueDist{
          }
        }
      }

      setCueElevation{
        command:t="cueAxisZ"

        actions{

          setCueElevation{
          }
        }
      }

      setSelectedTarget{
        command:t="switchToTarget"

        actions{

          setDetectedTargetActive{
          }
        }
      }

      centerZone{
        command:t="directionAxesReset"

        actions{

          setCueToBoresight{
          }

          setCenterAzimuth{
            source:t="designationCue"
          }

          setCenterElevation{
            source:t="designationCue"
          }

          updateActiveDetectedTarget{
          }
        }
      }
    }
  }

  parkAntenna{
    stateInit:t="init"

    transitions{

      checkAntennaParked{
        event:t="update"

        actions{

          checkInScanDirectionPhase{
            azimuth:r=0.0
            elevation:r=0.0
          }
        }
      }
    }
  }
}

