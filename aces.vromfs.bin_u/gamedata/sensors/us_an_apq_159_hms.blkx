type:t="radar"
name:t="AN/APQ-159"
showMissileLaunchZone:b=yes

transivers{

  common{
    sideLobesAttenuation:r=-20.0
    power:r=300.0
    band:i=8
    rcs:r=5.0
    range:r=30000.0
    rangeMax:r=75000.0
    timeGainControl:b=yes
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=6.0
      sideLobesSensitivity:r=-28.0
    }
  }
}

scanPatterns{

  search{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-40.0, 45.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=45.0
    period:r=2.0
    width:r=45.0
    barHeight:r=5.5
    barsCount:i=2
    rowMajor:b=yes
  }

  boresightLock{
    type:t="cone"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-40.0, 45.0
    period:r=0.1
    width:r=0.1
    indicate:b=yes
  }

  bvrLockSearch{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-40.0, 45.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=45.0
    period:r=0.2
    width:r=4.0
    barHeight:r=5.0
    barsCount:i=2
    rowMajor:b=no
    indicate:b=yes
  }

  track{
    type:t="no"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-40.0, 45.0
  }

  hmdLock{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-45.0, 60.0
    period:r=0.2
    width:r=0.75
    barHeight:r=1.5
    barsCount:i=2
    rowMajor:b=yes
    indicate:b=yes
    hmd:b=yes
  }

  hmdTrack{
    type:t="no"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-45.0, 60.0
    hmd:b=yes
  }
}

signals{

  search{
    dynamicRange:p2=40.0, 15.0
    groundClutter:b=yes
    aircraftAsTarget:b=yes
    friendFoeId:b=no

    distance{
      presents:b=yes
      minValue:r=500.0
      maxValue:r=75000.0
      width:r=300.0
    }
  }

  track{
    dynamicRange:p2=40.0, 13.0
    groundClutter:b=yes
    aircraftAsTarget:b=yes
    angularAccuracy:r=0.06
    distanceAccuracy:r=15.0
    track:b=yes

    distance{
      presents:b=yes
      minValue:r=200.0
      maxValue:r=20000.0
      width:r=300.0
    }
  }
}

scanPatternSets{

  search{
    scanPattern1:t="search"
  }

  acmLock{
    scanPattern1:t="boresightLock"
  }

  hmdLock{
    scanPattern1:t="hmdLock"
  }
}

scopeRangeSets{

  search{
    range1:r=18500.0
    range2:r=37000.0
    range3:r=74000.0
    range4:r=9250.0
  }

  acmLock{
    range1:r=18500.0
  }
}

