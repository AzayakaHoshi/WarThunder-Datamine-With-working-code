    barHeight:r=4.1
    barsCount:i=3
    rowMajor:b=yes
    barsOneWay:b=yes
  }

  searchMediumRadar{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 60.0
