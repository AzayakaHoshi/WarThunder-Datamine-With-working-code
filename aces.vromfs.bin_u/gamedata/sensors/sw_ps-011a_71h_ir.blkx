    pitchStabLimit:r=60.0
    period:r=0.8
    width:r=5.0
    barHeight:r=4.0
    barsCount:i=4
    rowMajor:b=yes
    barsOneWay:b=yes
  }

  boresightLockRadar{
    type:t="cone"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 60.0
    period:r=1.0
    width:r=3.3
    indicate:b=yes
  }

  designationLockRadar{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 60.0
    rollStabLimit:r=90.0
    pitchStabLimit:r=60.0
    period:r=0.5
    width:r=8.0
    barHeight:r=3.5
    barsCount:i=2
    rowMajor:b=no
    indicate:b=yes
  }

  trackRadar{
    type:t="no"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 60.0
  }

  searchIrst{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 30.0
    rollStabLimit:r=50.0
    pitchStabLimit:r=45.0
    period:r=5.0
    width:r=60.0
    barHeight:r=5.0
    barsCount:i=12
    rowMajor:b=yes
    barsOneWay:b=no
  }

  designationLockIrst{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 30.0
