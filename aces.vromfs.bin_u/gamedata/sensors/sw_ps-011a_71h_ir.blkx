    pitchStabLimit:r=60.0
    period:r=2.5
    width:r=30.0
    barHeight:r=4.0
    barsCount:i=4
    rowMajor:b=yes
    barsOneWay:b=yes
  }

  searchNarrowRadar{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-30.0, 60.0
