  bvrLockSearch{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=0.5
    width:r=8.0
    barHeight:r=2.5
    barsCount:i=2
    rowMajor:b=no
    indicate:b=yes
  }

  track{
    type:t="no"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
  }
}

