    pitchStabLimit:r=60.0
    period:r=1.0
    width:r=60.0
    barHeight:r=3.0
    barsCount:i=1
    rowMajor:b=yes
  }

  boresightLock{
    type:t="cone"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    period:r=1.0
    width:r=1.75
    indicate:b=yes
