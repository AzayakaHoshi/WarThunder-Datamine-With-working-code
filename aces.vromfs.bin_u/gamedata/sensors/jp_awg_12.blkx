scanPatterns{

  searchNarrow{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=1.0
    width:r=15.0
    barHeight:r=2.5
    barsCount:i=2
    rowMajor:b=yes
  }

  searchWide3Bars{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=3.0
    width:r=60.0
    barHeight:r=2.67
    barsCount:i=3
    rowMajor:b=yes
  }

  searchWide1Bar{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
