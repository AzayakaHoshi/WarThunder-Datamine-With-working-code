        stateFrom:t="search"
        event:t="update"
        stateTo:t="track"

        actions{

          scan{
            scanPattern:t="radarFastTws"
            ignoreOwnWeapon:b=yes
          }

          extrapolateTargetsOfInterest{
          }

          clearTargetsOfInterest{
            timeOut:r=2.0
          }

          setCueToActiveTargetOfInterest{
          }

          setCenterAzimuth{
            source:t="activeTargetOfInterest"
          }

          setDistGatePos{
            source:t="activeTargetOfInterest"
            width:r=0.0
          }
        }
      }

      detect{
        event:t="targetInSight"

        actions{

          setDistGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          setRelSpeedGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          detectTarget{
            ignoreOwnWeapon:b=yes
          }
        }
      }

      addTarget{
        stateFrom:t="search"
        event:t="targetDetected"

        actions{

          updateTargetOfInterest{
            limit:i=10
            posGateTimeInitial:p2=2.0, 4.0
            posGateRangeInitial:r=0.0
            posGateMaxTime:r=2.0
            posGateRange:p2=50.0, 2000.0
            dopplerSpeedGateRange:p2=60.0, 300.0
            dopplerSpeedGateMaxTime:r=1.0
          }

          updateActiveDetectedTarget{
          }

          setCueToActiveTargetOfInterest{
          }
        }
      }

      addTargetTrack{
        stateFrom:t="track"
        event:t="targetDetected"

        actions{

          updateTargetOfInterest{
            limit:i=10
            onlyExisting:b=yes
            posGateTimeInitial:p2=2.0, 4.0
            posGateRangeInitial:r=100.0
            posGateMaxTime:r=2.0
            posGateRange:p2=0.0, 1000.0
            dopplerSpeedGateMaxTime:r=1.0
          }

          updateActiveDetectedTarget{
          }

          setCueToActiveTargetOfInterest{
          }

          setCenterAzimuth{
            source:t="activeTargetOfInterest"
          }

          setDistGatePos{
            source:t="activeTargetOfInterest"
            width:r=0.0
          }
        }
      }

      switchSelectedTarget{
        command:t="switchTarget"

        actions{

          setNextDetectedTargetActive{
          }
        }
      }

      setCueAzimuth{
        command:t="cueAxisX"

        actions{

          setCueAzimuth{
          }
        }
      }

      setCueDist{
        command:t="cueAxisY"

        actions{

          setCueDist{
          }
        }
      }

      setCueElevation{
        command:t="cueAxisZ"

        actions{

          setCueElevation{
          }
        }
      }

      setSelectedTarget{
        command:t="switchToTarget"

        actions{

          setDetectedTargetActive{
          }
        }
      }
    }
  }
}