type:t="radar"
name:t="9K330 TT"
weaponTargetsMax:i=2

transivers{

  radarTrack{
    power:r=600.0
    band:i=9
    rcs:r=3.0
    range:r=20000.0
    rangeMax:r=30000.0

    antenna{
      angleHalfSens:r=1.0
      sideLobesSensitivity:r=-40.0
    }
  }

  irstTrack{
    power:r=200.0
    band:i=-1
    rcs:r=28000.0
    range:r=8000.0
    rangeMax:r=10000.0
    visibilityType:t="optic"

    antenna{
      angleHalfSens:r=0.4
      sideLobesSensitivity:r=-120.0
    }
  }
}

scanPatterns{

  radarLockNarrow{
