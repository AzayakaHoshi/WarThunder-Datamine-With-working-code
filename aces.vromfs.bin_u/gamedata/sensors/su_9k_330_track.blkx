    azimuthLimits:p2=-30.0, 30.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=0.25
    width:r=3.5
    barHeight:r=1.0
    barsCount:i=3
    rowMajor:b=no
    indicate:b=yes
    preciseMinor:b=yes
  }

  radarLockWide{
    type:t="pyramide"
    azimuthLimits:p2=-30.0, 30.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=2.0
    width:r=30.0
    barHeight:r=1.0
    barsCount:i=3
    rowMajor:b=no
    centerElevation:r=30.0
    indicate:b=yes
    preciseMinor:b=yes
  }

  radarLosLock{
    type:t="pyramide"
    azimuthLimits:p2=-30.0, 30.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=0.25
    width:r=1.0
    barHeight:r=1.0
    barsCount:i=2
    rowMajor:b=no
    indicate:b=yes
    preciseMinor:b=yes
  }

  radarTrack{
    type:t="pyramide"
    azimuthLimits:p2=-30.0, 30.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=1.0
    width:r=7.5
    barHeight:r=0.5
    barsCount:i=30
    rowMajor:b=yes
    preciseMinor:b=yes
    tws:b=yes
  }

  radarFastTws{
    type:t="pyramide"
    azimuthLimits:p2=-30.0, 30.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=0.025
    period:r=0.05
    width:r=7.5
    barHeight:r=0.5
    barsCount:i=30
    rowMajor:b=yes
    preciseMinor:b=yes
    tws:b=yes
  }

  irstLock{
    type:t="pyramide"
    azimuthLimits:p2=-20.0, 20.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=0.1
    width:r=5.0
    barHeight:r=0.4
    barsCount:i=2
    rowMajor:b=no
    indicate:b=yes
  }

  irstLosLock{
    type:t="pyramide"
    azimuthLimits:p2=-20.0, 20.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
    period:r=0.25
    width:r=0.4
    barHeight:r=0.4
    barsCount:i=2
    rowMajor:b=no
    preciseMinor:b=yes
    indicate:b=yes
  }

  irstTrack{
    type:t="no"
    azimuthLimits:p2=-20.0, 20.0
    elevationLimits:p2=-30.0, 60.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=60.0
    rollStabLimit:r=30.0
  }
}

signals{

  radarTrack{
    groundClutter:b=no
    angularAccuracy:r=0.018
    distanceAccuracy:r=7.0
    targetRadiusMin:r=0.7375
    absDopplerSpeed:b=yes
    track:b=yes

    distance{
      presents:b=yes
      minValue:r=100.0
      maxValue:r=30000.0
    }

    dopplerSpeed{
      presents:b=yes
      minValue:r=10.0
      maxValue:r=1000.0
      signalWidthMin:r=5.0
      width:r=0.0
    }
  }

  irstTrack{
    groundClutter:b=no
    rangeFinder:b=no
    angularAccuracy:r=0.01
    distanceAccuracy:r=2.0
    targetRadiusMin:r=0.2

    distance{
      presents:b=yes
      minValue:r=30.0
      maxValue:r=10000.0
    }

    dopplerSpeed{
      presents:b=no
    }
  }
}

transmitters{

