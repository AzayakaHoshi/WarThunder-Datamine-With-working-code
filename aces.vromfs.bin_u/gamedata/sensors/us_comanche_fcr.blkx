type:t="radar"
name:t="AN/APG-78"

transivers{

  GTM{
    sideLobesAttenuation:r=-20.0
    power:r=100.0
    band:i=10
    rcs:r=40.0
    range:r=6300.0
    rangeMax:r=8000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{

      azimuth{
        angleHalfSens:r=0.5
        sideLobesSensitivity:r=-50.0
      }

      elevation{
        angleHalfSens:r=20.0
        sideLobesSensitivity:r=-50.0
      }
    }
  }

  ATM{
    sideLobesAttenuation:r=-20.0
    power:r=100.0
    band:i=10
    rcs:r=3.0
    range:r=8000.0
    rangeMax:r=8000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{

      azimuth{
        angleHalfSens:r=0.5
        sideLobesSensitivity:r=-50.0
      }

      elevation{
        angleHalfSens:r=20.0
        sideLobesSensitivity:r=-50.0
      }
    }
  }

  MTI{
    sideLobesAttenuation:r=-20.0
    power:r=200.0
    band:i=10
    rcs:r=40.0
    range:r=8000.0
    rangeMax:r=10000.0
    timeGainControl:b=yes
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{

      azimuth{
        angleHalfSens:r=0.5
        sideLobesSensitivity:r=-50.0
      }

      elevation{
        angleHalfSens:r=20.0
        sideLobesSensitivity:r=-50.0
      }
    }
  }
}

scanPatterns{

  searchWideATM{
    type:t="cylinder"
    azimuthLimits:p2=-180.0, 180.0
    elevationLimits:p2=-25.0, 25.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=6.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchMediumATM{
    type:t="pyramide"
    azimuthLimits:p2=-180.0, 180.0
    azimuthLimits:p2=-180.0, 180.0
    elevationLimits:p2=-25.0, 25.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=3.0
    width:r=90.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchNarrowATM{
    type:t="pyramide"
    azimuthLimits:p2=-180.0, 180.0
    elevationLimits:p2=-25.0, 25.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=1.5
    width:r=45.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchZoomATM{
    type:t="pyramide"
    azimuthLimits:p2=-180.0, 180.0
    elevationLimits:p2=-25.0, 25.0
    yawStabLimit:r=180.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.75
    width:r=22.5
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchWideGTM{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=1.5
    width:r=45.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchMediumGTM{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.75
    width:r=22.5
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchNarrowGTM{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.5
    width:r=15.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchZoomGTM{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.25
    width:r=7.5
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchWideMTI{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=1.5
    width:r=45.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchMediumMTI{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.75
    width:r=22.5
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchNarrowMTI{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.5
    width:r=15.0
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }

  searchZoomMTI{
    type:t="pyramide"
    azimuthLimits:p2=-45.0, 45.0
    elevationLimits:p2=-25.0, 25.0
    pitchStabLimit:r=25.0
    rollStabLimit:r=20.0
    period:r=0.25
    width:r=7.5
    barHeight:r=20.0
    barsCount:i=1
    rowMajor:b=yes
    preciseMinor:b=yes
  }
}

signals{

  GTM{
    groundClutter:b=no
    groundMultipath:b=no
    aircraftAsTarget:b=no
    groundVehiclesAsTarget:b=yes
    angularAccuracy:r=0.0
    distanceAccuracy:r=1.0

    distance{
      presents:b=yes
      minValue:r=500.0
      maxValue:r=8000.0
      width:r=15.0
    }
  }

  ATM{
    groundClutter:b=no
    groundMultipath:b=no
    aircraftAsTarget:b=yes
    groundVehiclesAsTarget:b=no
    angularAccuracy:r=0.0
    distanceAccuracy:r=1.0

    distance{
      presents:b=yes
      minValue:r=500.0
      maxValue:r=8000.0
      width:r=15.0
    }
  }

  MTI{
    groundClutter:b=no
    aircraftAsTarget:b=yes
    groundVehiclesAsTarget:b=yes
    angularAccuracy:r=0.0
    distanceAccuracy:r=1.0
    absDopplerSpeed:b=yes
    mainBeamDopplerSpeed:b=yes

    distance{
      presents:b=yes
      minValue:r=500.0
      maxValue:r=8000.0
      width:r=15.0
    }

    dopplerSpeed{
      presents:b=yes
      minValue:r=10.0
      maxValue:r=1000.0
      signalWidthMin:r=5.0
      width:r=0.0
    }
  }
}

scanPatternSets{

  GTM{
    scanPattern1:t="searchWideGTM"
    scanPattern2:t="searchMediumGTM"
    scanPattern3:t="searchNarrowGTM"
    scanPattern4:t="searchZoomGTM"
  }

  MTI{
    scanPattern1:t="searchWideMTI"
    scanPattern2:t="searchMediumMTI"
    scanPattern3:t="searchNarrowMTI"
    scanPattern4:t="searchZoomMTI"
  }

  ATM{
    scanPattern1:t="searchWideATM"
    scanPattern2:t="searchMediumATM"
    scanPattern3:t="searchNarrowATM"
    scanPattern4:t="searchZoomATM"
  }
}

scopeRangeSets{

  search{
    range1:r=8000.0
    range2:r=4000.0
  }
}

fsms{

  main{
    stateInit:t="init"

    actionsTemplates{

      init{

        setEnabled{
          value:b=no
        }

        setCenterAzimuth{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="constant"
          value:r=-6.25
        }

        setScopeRangeSet{
          scopeRangeSet:t="search"
        }

        setTransiver{
          transiver:t="GTM"
        }

        setSignal{
          signal:t="GTM"
        }

        setFsmActive{
          fsm:t="mode"
          active:b=yes
        }
      }

      setStandbyMode{

        setModeName{
          name:t="standby"
        }

        setEnabled{
          value:b=no
        }

        setFsmActive{
          fsm:t="mode"
          active:b=no
        }
      }

      resetStandbyMode{
      }

      setSearchMode{

        setEnabled{
          value:b=yes
        }

        setFsmActive{
          fsm:t="search"
          active:b=yes
        }

        setFsmActive{
          fsm:t="mode"
          active:b=yes
        }

        setCueEnabled{
          value:b=yes
          updateActiveTargetUnderCue:b=yes
        }
      }

      resetSearchMode{

        clearTargets{
        }

        setFsmActive{
          fsm:t="search"
          active:b=no
        }

        setCueEnabled{
          value:b=no
        }
      }
    }

    transitions{

      init{
        stateFrom:t="init"
        event:t="init"
        stateTo:t="standby"

        actions{

          init{
          }

          setStandbyMode{
          }
        }
      }

      switchOn{
        stateFrom:t="standby"
        command:t="switch"
        event:t="enable"
        stateTo:t="search"

        actions{

          setSearchMode{
          }
        }
      }

      switchOff{
        stateFrom:t="search"
        command:t="switch"
        event:t="disable"
        stateTo:t="standby"

        actions{

          resetSearchMode{
          }

          setStandbyMode{
          }
        }
      }

      switchScanPattern{
        stateFrom:t="standby"
        stateFrom:t="search"
        command:t="scanPatternSwitch"

        actions{

          setNextScanPattern{
          }
        }
      }

      switchScopeRange{
        command:t="rangeSwitch"

        actions{

          setNextScopeRange{
          }
        }
      }
    }
  }

  search{
    stateInit:t="search"

    transitions{

      scan{
        event:t="update"

        actions{

          doCustomActionTemplate{
            fsm:t="search"
            name:t="scan"
          }
        }
      }

      detect{
        event:t="targetInSight"

        actions{

          setDistGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          setRelSpeedGatePos{
            source:t="continuousScale"
            width:r=0.0
          }

          detectTarget{
          }
        }
      }

      addTarget{
        event:t="targetDetected"

        actions{

          addTarget{
          }

          updateActiveDetectedTarget{
          }
        }
      }

      switchSelectedTarget{
        command:t="switchTarget"

        actions{

          setNextDetectedTargetActive{
          }
        }
      }

      setCueAzimuth{
        command:t="cueAxisX"

        actions{

          setCueAzimuth{
          }
        }
      }

      setCueDist{
        command:t="cueAxisY"

        actions{

          setCueDist{
          }
        }
      }

      setCueElevation{
        command:t="cueAxisZ"

        actions{

          setCueElevation{
          }
        }
      }

      setSelectedTarget{
        command:t="switchToTarget"

        actions{

          setDetectedTargetActive{
          }
        }
      }

      designateTarget{
        command:t="selectTarget"

        actions{

          designateActiveDetectedTarget{
            type:t="target"
            sensorIndex:i=1
          }

          designateActiveDetectedTarget{
            type:t="target"
            self:b=yes
            sensorIndex:i=0
          }
        }
      }

      centerZone{
        command:t="directionAxesReset"

        actions{

          setCueToBoresight{
          }

          setCenterAzimuth{
            source:t="designationCue"
          }

          setCenterElevation{
            source:t="designationCue"
          }

          updateActiveDetectedTarget{
          }
        }
      }
    }

    actionsTemplates{

      scanGTM{

        scan{
        }

        setCenterAzimuth{
          source:t="constant"
          value:r=0.0
        }

        setCenterAzimuth{
          source:t="designationCue"
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="designationCue"
        }
      }

      scanMTI{

        scan{
        }

        setCenterAzimuth{
          source:t="constant"
          value:r=0.0
        }

        setCenterAzimuth{
          source:t="designationCue"
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="designationCue"
        }
      }

      scanATM{

        setCenterAzimuth{
          source:t="body"
        }

        scan{
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setCenterElevation{
          source:t="designationCue"
        }
      }
    }
  }

  mode{
    stateInit:t="init"

    actionsTemplates{

      setGTMMode{

        setModeName{
          name:t="ground_search"
        }

        setTransiver{
          transiver:t="GTM"
        }

        setSignal{
          signal:t="GTM"
        }

        setScanPatternSet{
          scanPatternSet:t="GTM"
        }

        setCenterElevation{
          source:t="constant"
          value:r=-6.25
        }

        setTargetDesignationRange{
          azimuthRange:p2=-45.0, 45.0
          azimuthRelWidthMin:r=0.03
          elevationRange:p2=-20.0, 20.0
          distanceRange:p2=500.0, 8000.0
          fromBody:b=no
        }

        setCustomActionTemplate{
          fsm:t="search"
          name:t="scan"
          actionTemplateName:t="scanGTM"
        }

        clearTargets{
        }
      }

      setATMMode{

        setModeName{
          name:t="air_search"
        }

        setTransiver{
          transiver:t="ATM"
        }

        setSignal{
          signal:t="ATM"
        }

        setScanPatternSet{
          scanPatternSet:t="ATM"
        }

        setCenterElevation{
          source:t="constant"
          value:r=0.0
        }

        setTargetDesignationRange{
          azimuthRange:p2=-180.0, 180.0
          azimuthRelWidthMin:r=0.03
          elevationRange:p2=-20.0, 20.0
          distanceRange:p2=500.0, 8000.0
          fromBody:b=yes
        }

        setCustomActionTemplate{
          fsm:t="search"
          name:t="scan"
          actionTemplateName:t="scanATM"
        }

        clearTargets{
        }
      }

      setMTIMode{

        setModeName{
          name:t="MTI search"
        }

        setTransiver{
          transiver:t="MTI"
        }

        setSignal{
          signal:t="MTI"
        }

        setScanPatternSet{
          scanPatternSet:t="MTI"
        }

        setCenterElevation{
          source:t="constant"
          value:r=-6.25
        }

        setTargetDesignationRange{
          azimuthRange:p2=-45.0, 45.0
          azimuthRelWidthMin:r=0.03
          elevationRange:p2=-20.0, 20.0
          distanceRange:p2=500.0, 8000.0
          fromBody:b=no
        }

        setCustomActionTemplate{
          fsm:t="search"
          name:t="scan"
          actionTemplateName:t="scanGTM"
        }

        clearTargets{
        }
      }
    }

    transitions{

      init{
        stateFrom:t="init"
        event:t="fsmActivate"
        stateTo:t="mti"

        actions{

          setGTMMode{
          }
        }
      }

      showGTM{
        stateFrom:t="gtm"
        event:t="fsmActivate"
        stateTo:t="gtm"

        actions{

          setGTMMode{
          }
        }
      }

      showATM{
        stateFrom:t="atm"
        event:t="fsmActivate"
        stateTo:t="atm"

        actions{

          setATMMode{
          }
        }
      }

      showMTI{
        stateFrom:t="mti"
        event:t="fsmActivate"
        stateTo:t="mti"

        actions{

          setMTIMode{
          }
        }
      }

      switchToATM{
        stateFrom:t="mti"
        command:t="modeSwitch"
        stateTo:t="atm"

        actions{

          setATMMode{
          }
        }
      }

      switchToGTM{
        stateFrom:t="atm"
        command:t="modeSwitch"
        stateTo:t="gtm"

        actions{

          setGTMMode{
          }
        }
      }

      switchToMTI{
        stateFrom:t="gtm"
        command:t="modeSwitch"
        stateTo:t="mti"

        actions{

          setMTIMode{
          }
        }
      }
    }
  }
}