transivers{

  mprf{
    sideLobesAttenuation:r=-20.0
    power:r=1000.0
    band:i=8
    rcs:r=3.0
    range:r=40000.0
    rangeMax:r=50000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=3.0
      sideLobesSensitivity:r=-35.0
    }
  }

  hprf{
    sideLobesAttenuation:r=-20.0
    power:r=1000.0
    band:i=8
    rcs:r=3.0
    range:r=100000.0
    rangeMax:r=120000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=3.0
      sideLobesSensitivity:r=-35.0
    }
  }

  irst{
    power:r=50.0
    band:i=1
    rcs:r=24000.0
    range:r=22000.0
    range1:r=6000.0
    rangeMax:r=100000.0
    visibilityType:t="infraRed"

    antenna{

      azimuth{
        angleHalfSens:r=3.0
        sideLobesSensitivity:r=-120.0
      }

      elevation{
        angleHalfSens:r=0.3
        sideLobesSensitivity:r=-120.0
      }
    }
  }
}

illuminationTransmitter{
