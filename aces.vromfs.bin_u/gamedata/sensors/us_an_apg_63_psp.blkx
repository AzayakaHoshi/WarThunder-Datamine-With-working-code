          active:b=no
        }
      }

      setRam{

        setCueEnabled{
          value:b=yes
          updateActiveTargetUnderCue:b=yes
          keepActiveTarget:b=yes
        }

        setScanPattern{
          scanPattern:t="ram"
        }

        setFsmActive{
          fsm:t="ram"
          active:b=yes
        }
      }

      resetRam{

        setFsmActive{
          fsm:t="ram"
          active:b=no
        }
      }
