        actions{

          setDetectedTargetActive{
          }
        }
      }

      designateTarget{
        command:t="selectTarget"

        actions{

          designateActiveDetectedTarget{
            type:i=0
            sensorIndex:i=1
          }

          designateTargetUnderCue{
            type:i=0
            sensorIndex:i=1
          }
        }
      }

      activate{
        event:t="fsmActivate"

        actions{

          setTransiver{
            transiver:t="mprf"
          }

          setSignal{
            signal:t="mprfTrack"
          }

          setModeName{
            name:t="PD RAM"
          }
        }
      }
    }
