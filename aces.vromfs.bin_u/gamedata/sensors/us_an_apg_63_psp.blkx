type:t="radar"
name:t="AN/APG-63"
showMissileLaunchZone:b=yes
showMissileDgftLaunchZone:b=yes
showMissileAimLead:b=yes

transivers{

  hprf{
    sideLobesAttenuation:r=-20.0
    power:r=2000.0
    band:i=8
    rcs:r=3.0
    range:r=100000.0
    rangeMax:r=120000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=3.0
      sideLobesSensitivity:r=-35.0
    }
  }

  hprfVelocity{
    sideLobesAttenuation:r=-20.0
    power:r=2000.0
    band:i=8
    rcs:r=3.0
    range:r=120000.0
    rangeMax:r=140000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=3.0
      sideLobesSensitivity:r=-35.0
    }
  }

  mprf{
    sideLobesAttenuation:r=-20.0
    power:r=1000.0
    band:i=8
    rcs:r=5.0
    range:r=50000.0
    rangeMax:r=80000.0
    multipathEffect:p4=0.0, 1.0, 60.0, 0.0

    antenna{
      angleHalfSens:r=3.0
      sideLobesSensitivity:r=-35.0
    }
  }
}

illuminationTransmitter{
  power:r=2000.0

  antenna{
    angleHalfSens:r=3.0
    sideLobesSensitivity:r=-35.0
  }
}

scanPatterns{

  searchNarrow{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=0.85
    width:r=15.0
    barHeight:r=2.5
    barsCount:i=2
    rowMajor:b=yes
  }

  searchMedium{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=3.4
    width:r=30.0
    barHeight:r=2.5
    barsCount:i=4
    rowMajor:b=yes
  }

  searchWide{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=6.8
    width:r=60.0
    barHeight:r=2.5
    barsCount:i=4
    rowMajor:b=yes
  }

  bvrLockSearch{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    rollStabLimit:r=70.0
    pitchStabLimit:r=60.0
    period:r=0.34
    width:r=3.0
    barHeight:r=2.0
    barsCount:i=4
    rowMajor:b=yes
    indicate:b=yes
  }

  boresightLock{
    type:t="cone"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    period:r=0.3
    width:r=1.0
    centerElevation:r=0.0
    indicate:b=yes
  }

  verticalLock{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    period:r=1.8
    width:r=20.0
    barHeight:r=1.5
    barsCount:i=4
    centerElevation:r=25.0
    rowMajor:b=no
    indicate:b=yes
  }

  ssLock{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    period:r=1.3
    width:r=10.0
    barHeight:r=3.33
    barsCount:i=6
    rowMajor:b=yes
    indicate:b=yes
  }

  track{
    type:t="no"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
  }

  ram{
    type:t="pyramide"
    azimuthLimits:p2=-60.0, 60.0
    elevationLimits:p2=-60.0, 60.0
    period:r=0.2
    width:r=7.5
    barHeight:r=2.5
    barsCount:i=2
    rowMajor:b=yes
    preciseMinor:b=yes
  }
}

