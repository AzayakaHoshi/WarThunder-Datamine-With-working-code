Weapon{
  trigger:t="gunner2"
  triggerGroup:t="torpedoes"
  external:b=yes
  blk:t="gameData/Weapons/torpedoes/ussr_400mm_set40_torpedo.blk"
  emitter:t="bone_torpedo_01"
  bullets:i=5
  useEmitter:b=yes
  emitterGenRange:ip2=1, 5
  emitterGenFmt:t="bone_torpedo_%02d"
  ammoDpGenFmt:t="torpedo_%02d_dm"
  barrelDP:t="tt_01_dm"
  barrelDP:t="tt_01_dm"
  brokenBarrelAllowShoot:b=no
  brokenBarrelAllowShoot:b=no
  enableCourseCorrection:b=no
  startFx:t="start_torpedo"
  startFxOffset:p3=4.0, 0.0, 0.0
  startSpeed:r=20.0
  useStartSpeed:b=yes
  allowableDelta:r=40.0

  turret{
    head:t="tt_head_01"
    gun:t="tt_01"
  }

  limits{
    yaw:p2=-180.0, 180.0
    pitch:p2=0.0, 0.0
  }

  limitsDeadzone{
    yaw:p2=-180.0, -145.0
    pitch:p2=0.0, 0.0
  }

  limitsDeadzone{
    yaw:p2=-50.0, 50.0
    pitch:p2=0.0, 0.0
  }

  limitsDeadzone{
    yaw:p2=145.0, 180.0
    pitch:p2=0.0, 0.0
  }
}

