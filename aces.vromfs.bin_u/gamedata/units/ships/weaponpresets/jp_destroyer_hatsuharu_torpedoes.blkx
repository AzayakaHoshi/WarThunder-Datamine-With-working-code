Weapon{
  trigger:t="gunner5"
  triggerGroup:t="torpedoes"
  external:b=yes
  blk:t="gameData/Weapons/torpedoes/jp_610mm_type90_torpedo.blk"
  emitter:t="bone_torpedo_01"
  bullets:i=6
  useEmitter:b=yes
  emitterGenRange:ip2=1, 3
  emitterGenFmt:t="bone_torpedo_%02d"
  ammoDpGenFmt:t="torpedo_%02d_dm"
  barrelDP:t="tt_01_dm"
  barrelDP:t="tt_01_dm"
  brokenBarrelAllowShoot:b=no
  brokenBarrelAllowShoot:b=no
  enableCourseCorrection:b=no
  startFx:t="start_torpedo"
  startFxOffset:p3=4.0, 0.0, 0.0
  startSpeed:r=20.0
  useStartSpeed:b=yes
  allowableDelta:r=40.0

  turret{
    head:t="tt_head_01"
    gun:t="tt_01"
    gunnerDm:t="tt_01_dm"
  }

  limits{
    yaw:p2=-140.0, 140.0
    pitch:p2=0.0, 0.0
  }

  limitsDeadzone{
    yaw:p2=-40.0, 50.0
    pitch:p2=0.0, 0.0
  }

  limitsDeadzone{
    yaw:p2=130.0, 140.0
    pitch:p2=0.0, 0.0
  }
}

