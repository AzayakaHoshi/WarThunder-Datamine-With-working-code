sfxReloadBullet:t="grd_cannon_belt_reload"
isBulletBelt:b=no
notUseDefaultBulletInGui:b=yes

overheat{
  overheat:p2=5.0, 0.0
  overheat:p2=7.0, 0.1
  overheat:p2=9.0, 0.5
}

bullet{
  bulletCount:i=25
  mass:r=0.098
  caliber:r=0.025
  damageCaliber:r=0.009
  ballisticCaliber:r=0.009
  speed:r=1385.0
  Cx:r=0.18
  maxDistance:r=10000.0
  normalizationPreset:t="default"
  ricochetPreset:t="apds_fs_long"
