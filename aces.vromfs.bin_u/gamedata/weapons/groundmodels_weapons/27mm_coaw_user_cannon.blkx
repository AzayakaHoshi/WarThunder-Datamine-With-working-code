    ArmorPower1500m:p2=89.0, 1500.0
    ArmorPower2000m:p2=85.0, 2000.0
    ArmorPower3000m:p2=77.0, 3000.0
    ArmorPower10000m:p2=5.0, 10000.0
    ArmorPower20000m:p2=0.0, 20000.0
  }

  visual{
