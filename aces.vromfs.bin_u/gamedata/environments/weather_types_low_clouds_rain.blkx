clear{
  scatteringEffect:p2=1.0, 1.0
  cloud_shadow_intensity:r=0.55
  addWhiteTemp:r=0.0
  cloudDropletsScale:r=0.3
  cloudDropletsLayerThickness:r=2.0
  wind_strength:r=3.5
  waterWindStrength:r=3.0
  dustMul:r=1.0
  mudMul:r=1.0
  rainSplashEnabled:b=no
  rainSound:t="rain"
  rainCockpitSound:t="rain_cockpit"
  skyLightFactor:r=1.0
  cloudAircraftReflectMul:r=4.0
  cloudAircraftReflectPowerMul:r=0.5
  clouds_fly_cube_tex:t="cloud_fly_cube"
  cloudsShadowInfluence:r=0.97
  enableCloudsHole:b=no
  panoramaAllowed:b=no
  customWeatherTexture:t="low_clouds_rain_map"
  customWind:p2=70.0, 0.0
  forceFullresCloseClouds:b=yes

  clouds_weather_gen{
    "layers[0].coverage":p2=1.0, 1.0
    "layers[1].coverage":p2=0.45, 0.55
    "layers[0].seed":p2=0.0, 32768.0
    "layers[1].seed":p2=0.0, 32768.0
    epicness:r=0.0
  }

  clouds_form{
    "layers[0].startAt":p2=-0.2, -0.2
    "layers[0].thickness":p2=3.0, 3.0
    "layers[0].density":p2=0.95, 0.95
    "layers[0].clouds_type":p2=0.5, 0.5
    "layers[0].clouds_type_variance":p2=0.0, 0.0
    "layers[1].startAt":p2=1.5, 1.5
    "layers[1].thickness":p2=6.0, 6.0
    "layers[1].density":p2=0.95, 0.95
    "layers[1].clouds_type":p2=0.5, 0.5
    "layers[1].clouds_type_variance":p2=0.0, 0.0
    extinction:p2=1.5, 1.5
    turbulenceStrength:p2=11.0, 11.0
    shapeNoiseScale:i=7
    cumulonimbusShapeScale:i=4
    turbulenceFreq:i=7
  }

  render_panel{
    infinite_skies:b=yes
    cloudsSpeed:r=2000.0
    strataCloudsSpeed:r=10.0
    windDir:r=0.0
  }

  sky{
    mie_scale:p2=2.0, 2.2
    mie_height:p2=1.1, 2.02208
    mie2_thickness:r=422.0
    mie2_altitude:r=800.0
    mie2_scale:r=2.6
    multiple_scattering_factor:r=1.4
    moon_brightness:r=0.73
  }

  clouds_gen{
    humidity:p2=0.47, 0.62
    persistence:p2=0.58, 0.7
    asymmetry:p2=0.08, 0.22
    minCloudThickness:r=1.2
  }

  strata_clouds{
    amount:p2=0.9, 1.0
    altitude:p2=9.0, 11.9
  }

  windParams{
    cloudsWindSpeedMult:r=10000.0
    cloudsWindSpeedLimit:r=4000.0
    strataCloudsWindSpeedMult:r=4.0
    defaultWindSpeed:r=0.1
    height0:r=600.0
    turbulence0:r=0.7
    windMul0:r=1.0
    height1:r=2000.0
    turbulence1:r=0.7
    windMul1:r=1.0
    height2:r=8000.0
    turbulence2:r=0.3
    windMul2:r=0.01
  }

  rain{
    rainFromInvisibleClouds:b=yes
    probability:r=1.0
  }

  clouds_rendering{
    forward_eccentricity:r=0.73
    back_eccentricity:r=0.42
    forward_eccentricity_weight:r=0.75
    erosion_noise_size:r=42.0
    ambient_desaturation:r=0.5
    ms_contribution:r=0.7
    ms_attenuation:r=0.3
    ms_ecc_attenuation:r=0.3
    erosionWindSpeed:r=50.0
  }

  rainX7{
    alphaFadeSpeedBegin:r=20.0
    alphaFadeSpeedEnd:r=170.0
    length:r=0.75
    speed:r=8.5
    width:r=0.02
    density:r=2.0
    wind:r=1.0
    alpha:r=0.2
    lighting:p3=0.3, 0.3, 0.32
    perPassMul:r=5.0
  }

  rainDroplets{
    droplets_reflection_min:r=0.1
    droplets_reflection_max:r=0.3
    droplets_size_scale:p3=20.0, 20.0, 4.0
    droplets_speed:r=1.5
  }

  waterFlowmapParams{
    simulationSpeed:r=2.0
    crossfadeTime:r=4.0
  }

  puddlesPower{

    p{
      value:p2=0.0, 0.0
    }
  }
}

